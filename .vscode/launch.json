{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Build and debug (GDB)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceRoot}/dns",
			"args": ["-s1.1.1.1", "zakladna.eu"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "build",
			"miDebuggerPath": "/usr/bin/gdb"
		}
	]
}
