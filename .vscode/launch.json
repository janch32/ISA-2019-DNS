{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Build and debug (GDB)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceRoot}/dns",
			//"args": ["-r6", "-s1.1.1.1", "www.ietf.org"],
			"args": ["-rt", "-s1.1.1.1", "www.fit.vutbr..cz.asd"],
			//"args": ["-rx", "-s1.1.1.1", "77.78.99.203"],
			//"args": ["-rx", "-s8.8.8.8", "23.9.229.147"],
			//"args": ["-rx6", "-s1.1.1.1", "2607:f8b0:4004:814::200e"],
			//"args": ["-rx6", "-s8.8.8.8", "2a03:2880:f003:c07:face:b00c::2"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "build",
			"miDebuggerPath": "/usr/bin/gdb"
		}
	]
}
